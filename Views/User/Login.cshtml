<h1>Login</h1>

<form asp-action="Login" method="post">
    <div>
        <label>Email</label>
        <input name="email" type="email" />
    </div>
    <div>
        <label>Password</label>
        <input name="password" type="password" />
    </div>
    <button type="submit">Login</button>
</form>
